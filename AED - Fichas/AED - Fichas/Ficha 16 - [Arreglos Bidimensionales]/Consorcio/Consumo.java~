public class Consumo
{
    private float electricidad;
    private float gas;  
    private float agua;
    
    public Consumo()
    {
    }
    
    public Consumo(float e, float g, float a)
    {
       electricidad = e;
       gas = g;
       agua = a;
    }
    
    public float getGastoElectricidad()
    {
        return electricidad;
    }
    
    public float getGastoGas()
    {
        return gas;
    }

    public float getGastoAgua()
    {
        return agua;
    }
    
    public void setGastoElectricidad(float e)
    {
        electricidad = e;
    }
    
    public void setGastoGas(float g)
    {
        gas = g;
    }
    
    public void setGastoAgua(float a)
    {
        agua = a;
    }
    
    public String toString()
w    {
        String r = "Gasto en electricidad: " + electricidad;
        r += "\tEn gas: " + gas;
        r += "\tEn agua: " + agua;
        return r;
    }
}
